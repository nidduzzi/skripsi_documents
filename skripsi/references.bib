@article{omitaomuOnLinePredictionMotor2006,
  title = {On-{{Line Prediction}} of {{Motor Shaft Misalignment Using Fast Fourier Transform Generated Spectra Data}} and {{Support Vector Regression}}},
  author = {Omitaomu, Olufemi A. and Jeong, Myong K. and Badiru, Adedeji B. and Hines, J. Wesley},
