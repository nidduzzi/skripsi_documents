@article{lepageImpactWeatherActivities2021,
  title = {Impact of {{Weather}}, {{Activities}}, and {{Service Disruptions}} on {{Transportation Demand}}},
  author = {Lepage, Simon and Morency, Catherine},
  date = {2021-01},
  journaltitle = {Transportation Research Record: Journal of the Transportation Research Board},
  shortjournal = {Transportation Research Record},
  volume = {2675},
