@article{reinhardtAsteroidRiskAssessment2016,
  title = {Asteroid {{Risk Assessment}}: {{A Probabilistic Approach}}},
  shorttitle = {Asteroid {{Risk Assessment}}},
  author = {Reinhardt, Jason C. and Chen, Xi and Liu, Wenhao and Manchev, Petar and Paté‐Cornell, M. Elisabeth},
  date = {2016-02},
  journaltitle = {Risk Analysis},
